{
    "manifest_version": 2,
    "name": "蘋果日報小助手",
    "version": "1.1",
    "description": "蘋果日報網路訂閱制上路，免註冊免登入無料看到飽",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "background": {
        "scripts": [
            "blocking.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "https://*.appledaily.com/*"
            ],
            "run_at": "document_end",
            "js": [
                "jquery.js",
                "show.js"
            ]
        }
    ],
    "permissions": [
        "https://*.appledaily.com/*"
    ]
}